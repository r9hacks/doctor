<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pifss.doctor">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activitys.MainActivity" />
        <activity android:name=".Activitys.HomeReportActivity" />
        <activity
            android:name=".Activitys.RegisterActivity"
            android:exported="true" />
        <activity android:name=".Activitys.loginActivity" />
        <activity android:name=".Activitys.AboutUsActivity" />
        <activity android:name=".Activitys.DoctorProfileActivity" />
        <activity android:name=".Activitys.MyPatientActivity" />
        <activity android:name=".Activitys.PatientRequestActivity" />
        <activity android:name=".Activitys.SettingsActivity" />
        <activity android:name=".Activitys.PatientProfileActivity" />
        <activity android:name=".Activitys.ReportDetailActivity" />
        <activity android:name=".Activitys.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activitys.ForgetPasswordActivity" />
        <activity android:name=".Activitys.EditDoctorProfileActivity" />
        <activity android:name=".Activitys.ReplyReportActivity"></activity>
    </application>

</manifest>