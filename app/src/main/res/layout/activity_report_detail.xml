            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="12dp"
            android:layout_marginStart="12dp"
            android:layout_toEndOf="@+id/imageViewBlood"
            android:layout_toRightOf="@+id/imageViewBlood"
            android:text="Blood Pressure" />

        <TextView
            android:id="@+id/textViewbloodContent"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageViewBlood"
            android:layout_alignEnd="@+id/textViewblood"
            android:layout_alignLeft="@+id/textViewblood"
            android:layout_alignRight="@+id/textViewblood"
            android:layout_alignStart="@+id/textViewblood"
            android:layout_below="@+id/textViewblood"
            android:gravity="center_vertical"
            android:text="High"
            android:textSize="18sp" />
    </RelativeLayout>

