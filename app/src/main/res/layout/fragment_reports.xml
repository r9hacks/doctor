<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.pifss.doctor.Fragments.ReportsFragment">

    <!-- TODO: Update blank fragment layout -->
    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.pifss.doctor.Navigation">
        <android.support.design.widget.TabLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/mytabLayout"
            android:layout_below="@+id/myToolbar"
            android:layout_alignParentLeft="true"
            android:background="@color/blueActionBarColor"
            app:tabSelectedTextColor="@color/TapSelectedColor"
            app:tabTextColor="@color/TapNotSelectedColor"
            android:layout_alignParentStart="true">

        </android.support.design.widget.TabLayout>

        <android.support.v4.view.ViewPager
            android:id="@+id/myViewPager"
            android:layout_width="match_parent"
            android:layout_height="403dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"

            android:layout_below="@+id/mytabLayout"></android.support.v4.view.ViewPager>

    </RelativeLayout>

</FrameLayout>
